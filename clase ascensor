package panelseleccion;

/**
 *
 * @author AdanSutro
 */
public class Ascensor {
//atributo 
int piso=1;

//metodo cambiarpiso 
public void cambiarpiso(int valor)
{piso=valor;
}// fin metodo

//metodo ver piso actual con retorno
public int verpisoactual()
{return piso; 
}// fin metodo

}
